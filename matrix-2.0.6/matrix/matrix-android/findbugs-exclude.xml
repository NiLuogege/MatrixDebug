<FindBugsFilter>

    <Match>
        <Class name="~.*R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*Manifest\$.*"/>
    </Match>
    <Match>
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD, DM_DEFAULT_ENCODING, DM_GC, ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD"/>
    </Match>

</FindBugsFilter>